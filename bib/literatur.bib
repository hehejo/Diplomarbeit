  posted-at = {2009-03-31 16:47:42},
  priority = {2},
  url = {http://dx.doi.org/10.1145/1326257.1326278}
}

@INPROCEEDINGS{citeulike:4243165,
  author = {Webb, Steven D. and Soh, Sieteng and Trahan, Jerry},
  title = {Secure Referee Selection for Fair and Responsive Peer-to-Peer Gaming},
  booktitle = {PADS '08: Proceedings of the 22nd Workshop on Principles of Advanced
	and Distributed Simulation},
  year = {2008},
  pages = {63--71},
  address = {Washington, DC, USA},
  publisher = {IEEE Computer Society},
  abstract = {Peer-to-peer (P2P) architectures for Massively Multiplayer Online
	Games (MMOG) provide better scalability than Client/Server (C/S);
	however, they increase the possibility of cheating. Recently proposed
	P2P protocols use trusted referees that simulate/validate the game
	to provide security equivalent to C/S. When selecting referees from
	un-trusted peers, selecting non-colluding referees becomes critical.
	Further, referees should be selected such that the range and length
	of delays to players is minimised (maximising game fairness and responsiveness).
	In this paper we formally define the referee selection problem and
	propose two secure referee selection algorithms, SRS-1 and SRS-2,
	to solve it. Both algorithms ensure the probability of corrupt referees
	controlling a zone/region is below a pre-defined limit, while attempting
	to maximise responsiveness and fairness. The trade-off between responsiveness
	and fairness is adjustable for both algorithms. Simulations show
	the effectiveness of our algorithms in two different scenarios.},
  citeulike-article-id = {4243165},
  citeulike-linkout-0 = {http://portal.acm.org/citation.cfm?id=1381309.1382287},
  citeulike-linkout-1 = {http://dx.doi.org/10.1109/PADS.2008.16},
  doi = {10.1109/PADS.2008.16},
  isbn = {978-0-7695-3159-5},
  keywords = {mmog, p2p, peer-to-peer},
  posted-at = {2009-03-31 16:36:54},
  priority = {3},
  url = {http://dx.doi.org/10.1109/PADS.2008.16}
}

