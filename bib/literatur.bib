@ARTICLE{Liebeherr2002Applicationlayer,
  author = {Liebeherr, J. and Nahas, M. and Si, Weisheng},
  title = {Application-layer multicasting with Delaunay triangulation overlays},
  journal = {IEEE Journal on Selected Areas in Communications},
  year = {2002},
  volume = {20},
  pages = {1472--1488},
  number = {8},
  month = {October},
  abstract = {Application-layer multicast supports group applications without the
	need for a network-layer multicast protocol. Here, applications arrange
	themselves in a logical overlay network and transfer data within
	the overlay. We present an application-layer multicast solution that
	uses a Delaunay triangulation as an overlay network topology. An
	advantage of using a Delaunay triangulation is that it allows each
	application to locally derive next-hop routing information without
	requiring a routing protocol in the overlay. A disadvantage of using
	a Delaunay triangulation is that the mapping of the overlay to the
