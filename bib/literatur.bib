@MISC{citeulike:1953628,
  author = {Kabus, Patric and Terpstra, Wesley W. and Cilia, Mariano and Buchmann,
	Alejandro P.},
  title = {Addressing Cheating in Distributed MMOGs},
  abstract = {Massively Multiplayer Online Games (MMOGs) are a risky business: while
	they o\#er potential profits beyond those of conventional computer
	games, they also require costly investment in the necessary hardware
	infrastructure. In nearly every MMOG today, these costs come from
