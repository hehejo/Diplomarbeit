  isbn = {1-59593-336-0},
  keywords = {mmog, p2p, peer-to-peer},
  location = {Columbia, Maryland},
  posted-at = {2009-03-31 16:59:12},
  priority = {2},
  url = {http://dx.doi.org/10.1145/1122591.1122599}
}

