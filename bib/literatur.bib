  doi = {10.1109/VECIMS.2008.4592763},
  journal = {Virtual Environments, Human-Computer Interfaces and Measurement Systems,
	2008. VECIMS 2008. IEEE Conference on},
  pages = {111--115},
  posted-at = {2009-06-10 12:21:47},
