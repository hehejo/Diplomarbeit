  doi = {10.1109/HIPC.1997.634498},
  journal = {High-Performance Computing, 1997. Proceedings. Fourth International
	Conference on},
  keywords = {loadbalancing, mmog, partitioning},
  pages = {230--235},
  posted-at = {2009-04-07 13:20:09},
  priority = {3},
  url = {http://dx.doi.org/10.1109/HIPC.1997.634498}
}

