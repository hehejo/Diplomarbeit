@MISC{citeulike:282325,
  author = {And, Rodrigo R.},
  title = {High Availability in DHTs: Erasure Coding vs. Replication},
  abstract = {High availability in peer-to-peer DHTs requires data redundancy. This
	paper compares two popular redundancy schemes: replication and erasure
	coding. Unlike previous comparisons, we take the characteristics
	of the nodes that comprise the overlay into account, and conclude
	that in some cases the benefits from coding are limited, and may
