  isbn = {1-58113-987-X},
  keywords = {event, mmog, p2p},
  location = {Stevenson, Washington, USA},
  posted-at = {2010-02-04 09:06:11},
  priority = {2},
  url = {http://dx.doi.org/10.1145/1065983.1066005}
}

