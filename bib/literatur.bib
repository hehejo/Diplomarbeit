  isbn = {1-58113-987-X},
  keywords = {areaofinterestmanagement, mmog, p2p, peer-to-peer},
  location = {Stevenson, Washington, USA},
  posted-at = {2009-03-31 16:56:05},
  priority = {3},
  url = {http://dx.doi.org/10.1145/1065983.1066007}
}

@ARTICLE{citeulike:1824924,
