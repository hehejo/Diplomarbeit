@INPROCEEDINGS{citeulike:1953623,
  author = {Iimura, Takuji and Hazeyama, Hiroaki and Kadobayashi, Youki},
  title = {Zoned federation of game servers: a peer-to-peer approach to scalable
	multi-player online games},
  booktitle = {NetGames '04: Proceedings of 3rd ACM SIGCOMM workshop on Network
	and system support for games},
  year = {2004},
  pages = {116--120},
  address = {New York, NY, USA},
  publisher = {ACM},
  abstract = {Today's Multi-player Online Games (MOGs) are challenged by infrastructure
	requirements, because of their server-centric nature. Peer-to-peer
	networks are an interesting alternative, if they can implement the
	set of functions that are traditionally performed by centralized
	authoritative servers. In this paper, we propose a zoned federation
	model to adapt MOG to peer-to-peer networks. In this model, zoning
	layer is inserted between the game program and peer-to-peer networks.
	We introduce the concept of zone and zone owner to MOG. Zone is some
	part of the whole game world, and zone owner is an authoritative
	server of a specific zone. According to the demands of the game program,
	each node actively changes its role to zone owner and works in the
	same way as a centralized authoritative server. By dividing the whole
	game world into several zones, workloads of the centralized authoritative
	game server can be distributed to a federation of nodes. We have
	implemented the zoned federation model, and evaluate it with a prototypical
	multi-player game. Evaluation results indicate that our proposed
	approach is applicable to small and medium-sized MOGs, where the
	number of nodes is less than 500.},
  citeulike-article-id = {1953623},
  citeulike-linkout-0 = {http://portal.acm.org/citation.cfm?id=1016549},
  citeulike-linkout-1 = {http://dx.doi.org/10.1145/1016540.1016549},
  doi = {10.1145/1016540.1016549},
  isbn = {1-58113-942-X},
  location = {Portland, Oregon, USA},
  posted-at = {2010-02-04 08:57:24},
  priority = {2},
  url = {http://dx.doi.org/10.1145/1016540.1016549}
}

